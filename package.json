    "husky": "^7.0.4",
    "prettier": "^2.6.2",
    "prop-types": "^15.8.1",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.11.1",
    "webpack-merge": "^5.8.0",
    "webpack-userscript": "^3.2.2"
  },
